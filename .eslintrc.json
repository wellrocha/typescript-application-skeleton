{
    "env": {
        "browser": false,
        "es2021": true,
        "jest": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "tsconfig.eslint.json"
    },
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "rules": {}
}
